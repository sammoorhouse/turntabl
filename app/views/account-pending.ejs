<!DOCTYPE html>
<html lang="en">

<head>
  <% include partials/head.ejs %>
      <script>
  $(function(){highlightHeader('.header-pending')})

  <% if (typeof events != 'undefined') { %>

      var eventResources = [
      <% events.forEach(function(event){ %> {
        date: "<%= event.date %>",
        length: "<%= event.length %>",
        client: "<%= event.client %>",
        hourly rate: "<%= event.hourlyrate %>",
        completed: "<%= event.completed %>"
      },
      <%})%>

      <% } %>
  </script>
</head>

<body id="page-top">
  <% include partials/header_collapsed.ejs %>
  <div id="wrap">
    <div class="container m-t-3"></div>
    <div class="container">
      <h2>Pending sessions</h2>
      <div class="panel">
        <div class="row">
          <div class="col-lg-12 m-b-0">
            <div class="tile tile-inverse" tabindex="0">
              <div class="media">
                <div class="media-left text-info">
                  <i class="fa  fa-smile-o fa-3x fa-fw tt-icon" aria-hidden="true"></i>
                </div>
                <div class="media-body text-xs-left">
                  <h3 class="h5 m-b-0">You have no pending sessions</h3>
                  <p>Create a new mentoring session and start to make money! <a href="/account/main?modal=1">Set up a new session</a></p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% include partials/sitewide_footer.ejs %>
</body>

</html>
